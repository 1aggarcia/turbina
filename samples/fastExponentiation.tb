let MOD = 1000000007;

// Recursive implementation of the fast exponentiation algorithm
let pow(n: int, x: int): int ->
    if (x == 0) 1
    else if (x == 1) n
    else if (x == 2) n * n
    else (if (x % 2 == 0) 1 else n) * pow(n * n % MOD, x / 2);

println(pow(2, 8));
println(pow(5, 11));

// stack overflow
println(pow(50, 120));
